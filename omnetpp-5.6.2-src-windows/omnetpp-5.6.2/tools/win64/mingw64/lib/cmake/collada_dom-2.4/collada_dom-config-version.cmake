set( PACKAGE_VERSION "2.4.0" )
if( "${PACKAGE_FIND_VERSION}" VERSION_EQUAL "2.4.0")
  set(PACKAGE_VERSION_EXACT 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}.${PACKAGE_FIND_VERSION_MINOR}" EQUAL "2.4" )
  if( ${PACKAGE_FIND_VERSION_MINOR} GREATER 0 )
    set(PACKAGE_VERSION_COMPATIBLE 1)
  endif()
elseif( "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "2" )
  # for now backward compatible with most minor versions...
  if( ${PACKAGE_FIND_VERSION_MINOR} GREATER 0 )
    set(PACKAGE_VERSION_COMPATIBLE 1)
  endif()
endif()
